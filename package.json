{
  "name": "@hienfeld/model-creator",
  "version": "1.0.0",
  "description": "Comprehensive tool for integrating Git-based UML domain models with Confluence using SIVI AFD 2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "start": "node dist/index.js",
    "watch": "tsc --watch"
  },
  "keywords": [
    "uml",
    "domain-model",
    "confluence",
    "git",
    "sivi",
    "afd",
    "insurance",
    "mermaid",
    "plantuml"
  ],
  "author": "Hienfeld",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "commander": "^11.0.0",
    "form-data": "^4.0.3",
    "fs-extra": "^11.1.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mermaid": "^11.7.0",
    "moment": "^2.29.4",
    "node-fetch": "^3.3.0",
    "puppeteer": "^24.10.2",
    "simple-git": "^3.20.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.2.0"
  }
}
